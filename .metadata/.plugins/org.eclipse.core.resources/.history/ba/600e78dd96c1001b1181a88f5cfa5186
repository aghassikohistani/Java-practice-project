package duplicateCharcterPractice;

public class SwappingUsingThirdVariable {

	public static void main(String[] args) {
		int i = 10;
		int j = 20;
		int temp;
		System.out.println("BEfore Swap.. " + i + " " + j);
		temp = j;
		j = i;
		i = temp;
		System.out.println("After Swap.." + i + " " + j);
}

}
